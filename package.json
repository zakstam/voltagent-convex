{
  "name": "@o-zakstam/voltagent-convex",
  "description": "VoltAgent Convex - Convex storage adapter component for VoltAgent",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./api": {
      "import": {
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.js"
      },
      "require": {
        "types": "./dist/client/index.d.cts",
        "default": "./dist/client/index.cjs"
      }
    },
    "./convex.config": "./convex/convex.config.ts"
  },
  "files": [
    "dist",
    "convex"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "test": "vitest --reporter=hanging-process --reporter=default --typecheck",
    "test:coverage": "vitest run --coverage --typecheck",
    "prepublishOnly": "npm run build",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.10.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@voltagent/core": "^2.0.2",
    "@voltagent/logger": "^2.0.2",
    "ai": "^6.0.0",
    "convex": "^1.21.0",
    "semantic-release": "^25.0.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@voltagent/core": "^2.0.0",
    "ai": "^6.0.0",
    "convex": "^1.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakstam/voltagent-convex.git"
  },
  "keywords": [
    "voltagent",
    "convex",
    "ai",
    "agent",
    "memory",
    "storage",
    "component"
  ],
  "license": "MIT",
  "author": "",
  "bugs": {
    "url": "https://github.com/zakstam/voltagent-convex/issues"
  },
  "homepage": "https://github.com/zakstam/voltagent-convex#readme"
}
