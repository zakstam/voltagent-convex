{
  "name": "@o-zakstam/voltagent-convex",
  "description": "VoltAgent Convex - Convex storage adapter component for VoltAgent",
  "version": "1.1.3",
  "type": "module",
  "types": "./dist/client/index.d.ts",
  "module": "./dist/client/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./api": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./_generated/component.js": {
      "types": "./dist/component/_generated/component.d.ts"
    },
    "./convex.config": {
      "types": "./dist/component/convex.config.d.ts",
      "default": "./dist/component/convex.config.js"
    },
    "./convex.config.js": {
      "types": "./dist/component/convex.config.d.ts",
      "default": "./dist/component/convex.config.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc --project ./tsconfig.build.json",
    "dev:build": "chokidar 'tsconfig*.json' 'src/**/*.ts' -i '**/*.test.ts' -c 'convex codegen --component-dir ./src/component && npm run build' --initial",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "test": "vitest --reporter=hanging-process --reporter=default --typecheck",
    "test:coverage": "vitest run --coverage --typecheck",
    "prepare": "npm run build",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.10.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@voltagent/core": "^2.0.2",
    "@voltagent/logger": "^2.0.2",
    "ai": "^6.0.0",
    "chokidar-cli": "^3.0.0",
    "convex": "^1.21.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@voltagent/core": "^2.0.0",
    "ai": "^6.0.0",
    "convex": "^1.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakstam/voltagent-convex.git"
  },
  "keywords": [
    "voltagent",
    "convex",
    "ai",
    "agent",
    "memory",
    "storage",
    "component"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/zakstam/voltagent-convex/issues"
  },
  "homepage": "https://github.com/zakstam/voltagent-convex#readme"
}
